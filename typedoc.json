{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/index.ts",
    "src/tokenizer.ts",
    "src/utils/ast-tools.ts"
  ],
  "out": "docs/api",
  "plugin": [
    "typedoc-plugin-markdown"
  ],
  "readme": "none",
  "hideGenerator": true,
  "cleanOutputDir": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "githubPages": false
}
